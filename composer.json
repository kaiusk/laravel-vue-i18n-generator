{
    "name": "kaiusk/laravel-vue-i18n-generator",
    "license": "MIT",
    "description": "Generates a vue-i18n compatible include file from your Laravel translations.",
    "keywords": ["laravel","vue-i18n"],
    "homepage": "http://github.com/kaiusk/laravel-vue-i18n-generator",
    "authors": [
        {
            "name": "Martin Lindhe",
            "email": "martin@ubique.se"
        },
        {
            "name": "Kaius Karon",
            "email": "kaiuskaron@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "illuminate/console": "~6.0|~7.0|~8.0|~9.0",
        "illuminate/support": "~6.0|~7.0|~8.0|~9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7"
    },
    "autoload": {
        "psr-4": {
            "MartinLindhe\\VueInternationalizationGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MartinLindhe\\VueInternationalizationGenerator\\GeneratorProvider"
            ]
        }
    }
}
